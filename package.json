{
  "name": "node-tlcv",
  "version": "0.1.0",
  "description": "Backend Server for viewing TLCV chess games.",
  "author": "Jay Honnold <jayhonnold@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf build",
    "dev": "concurrently \"npm run dev-public\" \"npm run dev-server\"",
    "dev-public": "webpack --watch --progress --config webpack/webpack.dev.js",
    "dev-server": "nodemon src/server/main.ts",
    "format": "prettier --write --no-error-on-unmatched-pattern src public",
    "prebuild": "webpack --config webpack/webpack.prod.js",
    "lint": "eslint --fix src",
    "start": "node build/src/main.js",
    "test": "echo OK"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "chalk": "^4.1.2",
    "chess.js": "^1.0.0-beta.6",
    "chessboardjs": "^0.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.9",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.7",
    "express": "^4.21.2",
    "express-basic-auth": "^1.2.0",
    "jquery": "^3.6.0",
    "mini.css": "^3.0.1",
    "mkdirp": "^3.0.1",
    "on-finished": "^2.3.0",
    "reset-css": "^5.0.1",
    "serve-index": "^1.9.1",
    "slugify": "^1.6.6",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.7.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.0.7",
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.32",
    "@types/node": "^20.17.46",
    "@types/on-finished": "^2.3.1",
    "@types/serve-index": "^1.9.1",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.3",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "postcss-loader": "^7.3.3",
    "prettier": "~2.3.2",
    "raw-loader": "^4.0.2",
    "rimraf": "~3.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.9.0"
  }
}
